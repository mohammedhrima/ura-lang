func int to_ascii(char c):
    int n = c as int
    if n < 0 or n > 127: return -1
    // Check if it's a valid character
    if (n >= 97 and n <= 122) or  // a-z
       (n >= 65 and n <= 90) or   // A-Z
       (n >= 48 and n <= 57) or   // 0-9
       n == 32 or n == 0:         // space or null terminator
        return n
    return -1

func int strncmp(chars left, chars right, int n):
    int i = 0
    if n == 0: return 0
    
    while i < n and left[i] == right[i] and left[i] != '\0':
        i += 1
    
    if i == n:
        return 0
    
    int left_val = to_ascii(left[i])
    int right_val = to_ascii(right[i])
    return left_val - right_val

main():
    int len = strncmp("abd", "abc", 2) 
    return len