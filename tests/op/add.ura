func int foo():
    return 5

func int bar():
    return 10

main():
    int a = 10
    int b = 20
    
    // Test: variable + variable
    int t1 = a + b
    
    // Test: variable + value
    int t2 = a + 15
    
    // Test: variable + function
    int t3 = a + foo()
    
    // Test: value + variable
    int t4 = 25 + b
    
    // Test: value + value
    int t5 = 30 + 40
    
    // Test: value + function
    int t6 = 50 + bar()
    
    // Test: function + variable
    int t7 = foo() + a
    
    // Test: function + value
    int t8 = bar() + 100
    
    // Test: function + function
    int t9 = foo() + bar()