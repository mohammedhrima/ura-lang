fn to_ascii(c char) int:
    n int = c as int
    if n < 0 or n > 127: return -1
    // Check if it's a valid character
    if (n >= 97 and n <= 122) or  // a-z
       (n >= 65 and n <= 90) or   // A-Z
       (n >= 48 and n <= 57) or   // 0-9
       n == 32 or n == 0:         // space or null terminator
        return n
    return -1

fn strncmp(left chars, right chars, n int) int:
    i int = 0
    if n == 0: return 0
    while i < n and left[i] == right[i] and left[i] != '\0':
        i += 1
    if i == n: return 0
    left_val int = to_ascii(left[i])
    right_val int = to_ascii(right[i])
    return left_val - right_val

main():
    len int = strncmp("abd", "abc", 4) 
    return len