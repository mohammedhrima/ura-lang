func int foo():
    return 5

func int bar():
    return 10

main():
    int a = 8
    int b = 12
    
    // Test: variable < variable
    bool t1 = a < b
    
    // Test: variable < value
    bool t2 = a < 10
    
    // Test: variable < function
    bool t3 = a < foo()
    
    // Test: value < variable
    bool t4 = 5 < b
    
    // Test: value < value
    bool t5 = 3 < 7
    
    // Test: value < function
    bool t6 = 4 < bar()
    
    // Test: function < variable
    bool t7 = foo() < b
    
    // Test: function < value
    bool t8 = bar() < 15
    
    // Test: function < function
    bool t9 = foo() < bar()