fn foo() int:
    return 5

fn bar() int:
    return 10

main():
    a int = 23
    b int = 7
    
    // Test: variable % variable
    t1 int = a % b
    
    // Test: variable % value
    t2 int = a % 4
    
    // Test: variable % function
    t3 int = a % foo()
    
    // Test: value % variable
    t4 int = 50 % b
    
    // Test: value % value
    t5 int = 17 % 6
    
    // Test: value % function
    t6 int = 33 % foo()
    
    // Test: function % variable
    t7 int = bar() % b
    
    // Test: function % value
    t8 int = bar() % 3
    
    // Test: function % function
    t9 int = bar() % foo()
