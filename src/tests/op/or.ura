func bool foo():
    return True

func bool bar():
    return False

main():
    bool a = True
    bool b = False
    
    // Test: variable || variable
    bool t1 = a || b
    
    // Test: variable || value
    bool t2 = b || True
    
    // Test: variable || function
    bool t3 = b || foo()
    
    // Test: value || variable
    bool t4 = False || a
    
    // Test: value || value
    bool t5 = False || False
    
    // Test: value || function
    bool t6 = False || bar()
    
    // Test: function || variable
    bool t7 = bar() || a
    
    // Test: function || value
    bool t8 = foo() || False
    
    // Test: function || function
    bool t9 = bar() || foo()
    
    // Test using 'or' keyword
    bool t10 = a or b
    bool t11 = False or True
    bool t12 = bar() or foo()
    
    // Short-circuit evaluation tests
    // These should not call bar() if first operand is True
    bool t13 = True || bar()
    bool t14 = a or foo()