protoFunc float strtof(chars str, pointer endptr)
protoFunc float strtod(chars str, pointer endptr)
protoFunc long strtoll(chars str, pointer endptr, int base)
protoFunc long strtoull(chars str, pointer endptr, int base)

// Multibyte/wide character conversion
protoFunc int mblen(chars s, int n)
protoFunc int mbtowc(pointer pwc, chars s, int n)
protoFunc int wctomb(chars s, int wchar)
