// Program termination
protoFunc void abort()
protoFunc void exit(int code)
protoFunc void quick_exit(int code)
protoFunc void _Exit(int code)
protoFunc int atexit(pointer func)
protoFunc int at_quick_exit(pointer func)

// Environment
protoFunc chars getenv(chars name)
protoFunc int setenv(chars name, chars value, int overwrite)
protoFunc int unsetenv(chars name)
protoFunc int putenv(chars string)
protoFunc int system(chars command)

// Random numbers
protoFunc int rand()
protoFunc void srand(int seed)

// Searching and sorting
protoFunc pointer bsearch(pointer key, pointer base, int nmemb, int size, pointer compar)
