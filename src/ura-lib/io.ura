// File operations
proto fn fopen(path chars, mode chars) pointer
proto fn freopen(path chars, mode chars, file pointer) pointer
proto fn fclose(file pointer) int
proto fn fflush(file pointer) int

// Character I/O
proto fn fgetc(file pointer) int
proto fn fputc(c int, file pointer) int
proto fn getc(file pointer) int
proto fn putc(c int, file pointer) int
proto fn getchar() int
