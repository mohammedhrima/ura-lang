proto fn ungetc(c int, file pointer) int

// String I/O
proto fn fgets(buffer chars, size int, file pointer) chars
proto fn fputs(str chars, file pointer) int
proto fn puts(str chars) int
proto fn gets(buffer chars) chars

// Block I/O
proto fn fread(ptr pointer, size int, nmemb int, file pointer) int
proto fn fwrite(ptr pointer, size int, nmemb int, file pointer) int
